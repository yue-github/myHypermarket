.VerticalNav.nav {
  width: 200rpx;
  color: #666666;
  height: calc(100vh - (25px + 101rpx));
  white-space: initial;
}
.VerticalNav.nav .cu-item {
  width: 100%;
  text-align: center;
  background-color: #FAFAFA;
  margin: 0;
  border: none;
  height: 50px;
  position: relative;
}
.VerticalNav.nav .cu-item.cur {
  background-color: #FFFFFF;
}
.VerticalNav.nav .cu-item.cur::after {
  content: "";
  width: 7.27rpx;
  height: 32.72rpx;
  border-radius: 0px 9.09rpx 9.09rpx 0px;
  position: absolute;
  background-color: currentColor;
  top: 0;
  left: 0;
  bottom: 0;
  margin: auto;
}
.VerticalBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.VerticalMain {
  background-color: #FFFFFF;
  height: calc(100vh - (25px + 101rpx));
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.zaiui-grid-icon-box {
  position: relative;
  width: 100%;
}
.zaiui-grid-icon-box .cu-list.grid {
  background-color: inherit;
}
.zaiui-grid-icon-box .cu-list.grid .grid-icon {
  margin: 0;
}
.zaiui-grid-icon-box .cu-list.grid .grid-icon .icon {
  width: 127.27rpx;
}
.zaiui-grid-icon-box .cu-list.grid > .cu-item {
  padding-bottom: 9.09rpx;
}
.zaiui-grid-icon-box .cu-list.grid > .cu-item text {
  color: #333333;
}
.zaiui-grid-icon-box .cu-list.grid.no-border {
  padding: 0;
}

