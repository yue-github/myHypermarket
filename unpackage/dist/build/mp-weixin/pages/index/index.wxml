<view class="content"><block wx:for="{{data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="user-main"><image class="user-img" src="{{item.img}}" mode="widthFix" data-event-opts="{{[['tap',[['goOtherPage',[index]]]]]}}" bindtap="__e"></image><view class="user-content"><view>{{item.name+"："}}</view><view>{{item.age}}</view></view></view></block><view class="tag-view"><uni-tag vue-id="1" text="本地异步缓存" type="primary" data-event-opts="{{[['^click',[['bindClickPrimary']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag vue-id="2" text="地图选择位置" type="error" circle="{{true}}" data-event-opts="{{[['^click',[['bindClickError']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag vue-id="3" text="获取异步缓存" data-event-opts="{{[['^click',[['bindClickGray']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag style="margin-top:10rpx;" vue-id="4" text="{{requestText}}" type="warning" data-event-opts="{{[['^click',[['bindClickWarning']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag style="margin-top:10rpx;" vue-id="5" text="不同端打印不同的结果" type="warning" data-event-opts="{{[['^click',[['bindClickIf']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag style="margin-top:10rpx;" vue-id="6" text="utilRequest" type="warning" data-event-opts="{{[['^click',[['bindClickUtilRequest']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag style="margin-top:10rpx;" vue-id="7" text="异步封装调用" type="warning" data-event-opts="{{[['^click',[['bindClickApi']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><button style="margin-top:10px;" type="primary" loading="{{loading_is}}" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['getUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">微信授权</button><uni-tag vue-id="8" text="获取微信openid" type="primary" data-event-opts="{{[['^click',[['getOpenId']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><uni-tag style="margin-top:10rpx;" vue-id="9" text="vue到nvue跳转" type="primary" data-event-opts="{{[['^click',[['navigateToNvue']]]]}}" bind:click="__e" bind:__l="__l"></uni-tag><wxpay vue-id="10" bind:__l="__l"></wxpay></view><view class="uni-product-list"><block wx:for="{{productList}}" wx:for-item="product" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['showToast',[index]]]]]}}" class="uni-product" bindtap="__e"><view class="image-view"><block wx:if="{{renderImage}}"><image class="uni-product-image" src="{{product.image}}"></image></block></view><view class="uni-product-title">{{product.title}}</view><view class="uni-product-price"><text class="uni-product-price-favour">{{"￥"+product.originalPrice}}</text><text class="uni-product-price-original">{{"￥"+product.favourPrice}}</text><text class="uni-product-tip">{{product.tip}}</text></view></view></block></view><view class="uni-list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="uni-list-cell" hover-class="uni-list-cell-hover"><view class="uni-media-list"><image class="uni-media-list-logo" src="{{item.img}}"></image><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{item.title}}</view><view class="uni-media-list-text-bottom uni-ellipsis">{{item.content}}</view></view></view></view></block></view></view>